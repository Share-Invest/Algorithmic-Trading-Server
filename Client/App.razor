<Router AppAssembly="@typeof(App).Assembly">

    <Found Context="routeData">

        <RouteView RouteData="@routeData" DefaultLayout="@typeof(CoreLayout)" />

        <FocusOnNavigate RouteData="@routeData" Selector="h1" />

    </Found>

    <NotFound>

        <PageTitle>ShareInvest</PageTitle>

        <LayoutView Layout="@typeof(CoreLayout)">
            <iframe src="https://sharecompany.tistory.com/46" class="frame" allowfullscreen />
        </LayoutView>

    </NotFound>

</Router>